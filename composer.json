{
    "name": "brickx/laravel-filament-maintenance",
    "description": "Simple plugin to toggle maintenance mode from Filament",
    "keywords": [
        "BRICKX",
        "Laravel",
        "Filament",
        "Maintenance"
    ],
    "homepage": "https://github.com/brickx/laravel-filament-maintenance",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian PLAMONT",
            "email": "keysaw@brickx.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.11"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^2.6",
        "pestphp/pest-plugin-arch": "^2.1",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Brickx\\LaravelFilamentMaintenance\\": "src/",
            "Brickx\\LaravelFilamentMaintenance\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brickx\\LaravelFilamentMaintenance\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brickx\\LaravelFilamentMaintenance\\LaravelFilamentMaintenanceServiceProvider"
            ],
            "aliases": {
                "LaravelFilamentMaintenance": "Brickx\\LaravelFilamentMaintenance\\Facades\\LaravelFilamentMaintenance"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
